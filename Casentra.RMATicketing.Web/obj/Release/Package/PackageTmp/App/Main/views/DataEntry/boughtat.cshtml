
<div>
    <form name="createForm" role="form" novalidate class="form-validation">
        <div class="modal-header">
            <h4 class="modal-title">
                <span>Bought At</span>
            </h4>
        </div>
        <div class="modal-body">

            <div class="form-group">
                
               
                    <label>Bought At [English]</label>
                    <input auto-focus class="form-control" type="text" name="boughtatName" ng-model="vm.boughtat.name" required maxlength="50">
                
            
                    <label>Bought At [French]</label>
                    <input auto-focus class="form-control" type="text" name="boughtatName" ng-model="vm.boughtat.frenchName" required maxlength="50">
                
            </div>
            
            <div>
                <span class="help-block text-danger" ng-show="!createForm.boughtatName.$valid && createForm.boughtatName.$dirty">Invalid</span>
            </div>

            
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-primary" ng-click="vm.cancel()">@L("Cancel")</button>
            <button type="submit" class="btn btn-success " ng-click="vm.save()" ng-disabled="createForm.$invalid"><i class="fa fa-save"></i> <span>@L("Save")</span></button>
        </div>
    </form>
</div>